<h1>編集画面</h1>
<%= form_with model: @post, method: :patch do |f| %>
  <h4>フィットネスを行なった日</h4>
  <%= f.date_field :fitness_date %>
  <h4>メニュー</h4>
  <%= f.text_area :menu %>
  <h4>感想・メモ</h4>
  <%= f.text_area :body %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <%= f.submit '保存' %>
<% end %>